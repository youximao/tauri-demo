{
  "build": {
    "devPath": "../src/alwin/dist",
    "distDir": "../src/alwin/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tauri-alwin",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true,
        "open": true
      }
    },
    "windows": [
      {
        "title": "tauri-alwin",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": ["deb","dmg"],
      "identifier": "com.jia.alwin",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
